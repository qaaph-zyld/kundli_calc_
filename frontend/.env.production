# API Configuration
REACT_APP_API_URL=https://api.kundlicalculator.com
REACT_APP_API_VERSION=v1

# Feature Flags
REACT_APP_ENABLE_ADVANCED_FEATURES=true
REACT_APP_ENABLE_ANALYTICS=true

# Performance
REACT_APP_MAX_CACHE_SIZE=100
REACT_APP_CACHE_DURATION=3600

# Security
REACT_APP_CSP_ENABLED=true
REACT_APP_MAX_REQUESTS_PER_MIN=60

# Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_LOGGING_LEVEL=error

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.kundlicalculator.com
